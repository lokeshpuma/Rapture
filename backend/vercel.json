{
    "crons": [
    {
      "path": "/api/clear-uploads",
      "schedule": "0 2 * * *"
    }
    ],
    "version": 2,
    "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
    ],
    "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}
